@model IEnumerable<PhoneDictionary.Phone>

@{
    ViewBag.Title = "Index";
}

@section Header
{
    <header>
        <h2>HEADER</h2>
    </header>
}

<p>
    @Html.ActionLink("Create New", "Add")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Number)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        Html.RenderPartial("_Phone", item);
    }

</table>

@section Footer
{
    <header>
        <h2>FOOTER</h2>
    </header>
}